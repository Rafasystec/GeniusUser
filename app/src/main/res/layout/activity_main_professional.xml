<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="br.com.barcadero.geniususer.activities.MainProfessionalActivity">

    <FrameLayout
        android:id="@+id/frameMainProfessionalAgenda"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

<android.support.design.widget.BottomNavigationView
        android:id="@+id/btnNavigationPro"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:elevation="16dp"
        android:elevation="10dp"
        android:clickable="true"
        app:itemIconTint="@drawable/selector_item_menu"
        app:itemTextColor="@drawable/selector_item_menu"
        app:menu="@menu/bottom_navigation_professional" />

</RelativeLayout>
